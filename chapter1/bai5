import java.util.LinkedList;
public class bai5 {
    public static void main(String[] args){
        LinkedList<Integer> numbers = new LinkedList<>();
        numbers.add(10);
        numbers.add(20);
        numbers.add(30);
        System.out.println("Danh sach hien tai: " + numbers);

        numbers.addFirst(5);
        numbers.addLast(40);
        System.out.println("Danh sach sau khi them phan tu: " + numbers);

        numbers.removeFirst();
        numbers.removeLast();
        System.out.println("Danh sach sau khi xoa phan tu: " + numbers);
    }
}

